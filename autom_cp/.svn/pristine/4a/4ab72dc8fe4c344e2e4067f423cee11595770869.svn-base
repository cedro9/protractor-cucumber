    "wm-update-chrome-83": "webdriver-manager update --versions.chrome=83.0.4103.61",
    "wm-start": "webdriver-manager update && webdriver-manager start",
    "wm-clean": "webdriver-manager clean",
    "testcplh": "npm run protractorcuke -- --cucumberOpts.tags ~@bug --capabilities.chromeOptions.args=headless --capabilities.chromeOptions.args=disable-gpu --capabilities.chromeOptions.args=window-size=1600,2400",
    "testcpl": "npm run protractorcuke -- --cucumberOpts.tags ~@bug --capabilities.chromeOptions.args=start-maximized",
    "seb": "npm run testcplh -- --cucumberOpts.tags --suite seb,administracao,cadastro,operacoes,programacao",
