    "cuke": ".\\node_modules\\.bin\\cucumber-js --format=json:cucumber_report.json && node index.js",
    "protractorcuke": "protractor protractor.conf.js",
    "wm-update": "webdriver-manager update",
    "wm-update-chrome-latest": "npm run wm-update",
